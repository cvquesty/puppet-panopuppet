SQLITE_DIR: '<%= scope.lookupvar('::panopuppet::sqlite_dir') %>'
SECRET_KEY: '<%= scope.lookupvar('::panopuppet::secret_key') %>'
DEBUG: <%= scope.lookupvar('::panopuppet::debug') %>
TEMPLATE_DEBUG: <%= scope.lookupvar('::panopuppet::template_debug') %>
ALLOWED_HOSTS:
<% scope.lookupvar('::panopuppet::allowed_hosts').each do |aHost| %>
  - '<%= aHost %>'
<% end %>
LANGUAGE_CODE: "<%= scope.lookupvar('::panopuppet::django_lang_code') %>"
TIME_ZONE: "<%= scope.lookupvar('::panopuppet::django_time_zone') %>"
STATIC_ROOT: "<%= scope.lookupvar('::panopuppet::static_root') %>"

